<?xml version="1.0" encoding="ISO-8859-1"?>
<netprobe
	compatibility="1"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://schema.itrsgroup.com/GA2011.2-110303/netprobe.xsd"
>
	<selfAnnounce>
		<enabled>true</enabled>
		<retryInterval>60</retryInterval>
		<requireReverseConnection>true</requireReverseConnection>
		<probeName><hostname/><data>_INF_SA_</data><port/></probeName>
		<managedEntity>
			<name><hostname/><data>_INF_SA_</data><port/></name>
			<attributes>
				<attribute name="REGION">EU</attribute>
				<attribute name="DATACENTER">LDC</attribute>
				<attribute name="COUNTRY">UK</attribute>
				<attribute name="ENVIRONMENT">PROD</attribute>
				<attribute name="OS">LINUX</attribute>
				<attribute name="TYPE">INF</attribute>
				<attribute name="SERVER">PHYSICAL</attribute>
			</attributes>
			<types>
				<type>EMEA_LINUX_PROD</type>
				
			</types>
		</managedEntity>
		<gateways>
			<gateway>
				<hostname>ldc-lx-itrs03</hostname>
				<port>7037</port>
			</gateway>
			<gateway>
				<hostname>ldn-lx-itrs03</hostname>
				<port>7037</port>
			</gateway>
		</gateways>
	</selfAnnounce>
</netprobe>
